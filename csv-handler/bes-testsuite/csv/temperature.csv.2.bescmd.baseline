<?xml version="1.0" encoding="UTF-8"?>
<Dataset name="temperature.csv"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://xml.opendap.org/ns/DAP2"
xsi:schemaLocation="http://xml.opendap.org/ns/DAP2  http://xml.opendap.org/dap/dap2.xsd">


    <Array name="Station">
        <Attribute name="type" type="String">
            <value>String</value>
        </Attribute>
        <String/>
        <dimension name="record" size="5"/>
    </Array>
    <Array name="latitude">
        <Attribute name="type" type="Float32">
            <value>Float32</value>
        </Attribute>
        <Float32/>
        <dimension name="record" size="5"/>
    </Array>
    <Array name="longitude">
        <Attribute name="type" type="Float32">
            <value>Float32</value>
        </Attribute>
        <Float32/>
        <dimension name="record" size="5"/>
    </Array>
    <Array name="temperature_K">
        <Attribute name="type" type="Float32">
            <value>Float32</value>
        </Attribute>
        <Float32/>
        <dimension name="record" size="5"/>
    </Array>
    <Array name="Notes">
        <Attribute name="type" type="String">
            <value>String</value>
        </Attribute>
        <String/>
        <dimension name="record" size="5"/>
    </Array>

    <dataBLOB href=""/>
</Dataset>
