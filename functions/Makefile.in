install-lib_besLTLIBRARIES: $(lib_bes_LTLIBRARIES)
	@$(NORMAL_INSTALL)
	test -z "$(lib_besdir)" || $(MKDIR_P) "$(DESTDIR)$(lib_besdir)"
	@list='$(lib_bes_LTLIBRARIES)'; test -n "$(lib_besdir)" || list=; \
	list2=; for p in $$list; do \
	  if test -f $$p; then \
	    list2="$$list2 $$p"; \
	  else :; fi; \
	done; \
	test -z "$$list2" || { \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 '$(DESTDIR)$(lib_besdir)'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 "$(DESTDIR)$(lib_besdir)"; \
	}

uninstall-lib_besLTLIBRARIES:
	@$(NORMAL_UNINSTALL)
	@list='$(lib_bes_LTLIBRARIES)'; test -n "$(lib_besdir)" || list=; \
	for p in $$list; do \
	  $(am__strip_dir) \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f '$(DESTDIR)$(lib_besdir)/$$f'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f "$(DESTDIR)$(lib_besdir)/$$f"; \
	done

clean-lib_besLTLIBRARIES:
	-test -z "$(lib_bes_LTLIBRARIES)" || rm -f $(lib_bes_LTLIBRARIES)
	@list='$(lib_bes_LTLIBRARIES)'; for p in $$list; do \
	  dir="`echo $$p | sed -e 's|/[^/]*$$||'`"; \
	  test "$$dir" != "$$p" || dir=.; \
	  echo "rm -f \"$${dir}/so_locations\""; \
	  rm -f "$${dir}/so_locations"; \
	done
