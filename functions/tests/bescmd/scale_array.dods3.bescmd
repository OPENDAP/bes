<?xml version="1.0" encoding="UTF-8"?>
<bes:request xmlns:bes="http://xml.opendap.org/ns/bes/1.0#" reqID="[http-8080-1:27:bes_request]">
  <bes:setContext name="xdap_accept">3.2</bes:setContext>
  <bes:setContext name="dap_explicit_containers">no</bes:setContext>
  <bes:setContext name="errors">xml</bes:setContext>
  <bes:setContext name="max_response_size">0</bes:setContext>
  
  <bes:setContainer name="catalogContainer" space="catalog">/functions/tests/data/fnoc_hacked.dods</bes:setContainer>
  
  <bes:define name="d1" space="default">
    <bes:container name="catalogContainer">
      <!--  Int16 u[lat = 17][lon = 21]; u.u is 17 x 21 -->
      <bes:constraint>scale_array(u.u,u.lat,u.lon,17,21)</bes:constraint>
    </bes:container>
  </bes:define>
  
  <bes:get type="dods" definition="d1" />
  
</bes:request>
