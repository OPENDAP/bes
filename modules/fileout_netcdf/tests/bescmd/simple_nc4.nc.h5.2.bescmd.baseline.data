  	uint64 grp1_data(x, y) ;
  data:

   y = 0, 0, 0 ;

   grp1_data =
  0, 1, 2,
  3, 4, 5 ;
  } // group grp1

group: grp2 {
  dimensions:
  	y = 3 ;
