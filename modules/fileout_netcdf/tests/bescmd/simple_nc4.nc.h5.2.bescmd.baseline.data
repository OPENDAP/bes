netcdf test {
dimensions:
	y = 4 ;
	x = 2 ;
variables:
	float y(y) ;
	ushort data(x, y) ;
	float x(x) ;
data:

 y = 0, 0, 0, 0 ;

 data =
  0, 1, 2, 3,
  4, 5, 6, 7 ;

 x = 0, 0 ;

group: grp1 {
  dimensions:
  	y = 3 ;
  variables:
