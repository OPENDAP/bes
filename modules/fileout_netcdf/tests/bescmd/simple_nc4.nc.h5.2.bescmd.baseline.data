  	uint64 grp2_data(x, y) ;
  data:

   grp2_data =
  0, 1, 2,
  3, 4, 5 ;

  group: grp3 {
    dimensions:
    	z = 4 ;
    variables:
    	float z(z) ;
    	float grp3_data(x, /y, z) ;
    data:

     z = 0, 0, 0, 0 ;

     grp3_data =
  0, 1, 2, 3,
  1, 2, 3, 4,
  2, 3, 4, 5,
  3, 4, 5, 6,
  4, 5, 6, 7,
  5, 6, 7, 8,
  6, 7, 8, 9,
  7, 8, 9, 10 ;
    } // group grp3
  } // group grp2
}
