netcdf test {
dimensions:
	x = 2 ;
	y = 4 ;
variables:
	ushort data(x, y) ;

group: grp1 {
  dimensions:
  	y = 3 ;
  variables:
  	uint64 grp1_data(x, y) ;
  } // group grp1

group: grp2 {
  dimensions:
  	y = 3 ;
  variables:
  	uint64 grp2_data(x, y) ;

  group: grp3 {
    dimensions:
    	z = 4 ;
    variables:
    	float grp3_data(x, /y, z) ;
    } // group grp3
  } // group grp2
}
