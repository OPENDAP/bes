  group: Reflectivity {
    variables:
    	double d_reflctUC(DS_UTCTime_40) ;
    		d_reflctUC:hertz = "40" ;
    		d_reflctUC:long_name = "reflctUC" ;
    		d_reflctUC:description = "Reflectivity, not corrected for atmospheric effects, is calculated as Refl = R/T, where R is the received energy after it has been scaled for range, and T is the transmitted energy. i_reflctUC has also been calibrated for gain non-linearity (only for non-saturated waveforms), ground truth calibration and boresight shift shadowing (BSS). It is not corrected for saturation effects. If the shot is saturated (satindex above 2) then to correct for saturation the reflectivity estimate needs to be multiplied by the ratio of the corrected energy to the uncorrected energy (sat corrected reflectivity = d_reflctUC * (d_RecNrgAll + d_satNrgCorr)/d_RecNrgAll). The atmospheric corrected reflectivity may be calculated from this uncorrected reflectivity by multiplying it by d_reflCor_atm.  d_reflctUC is invalid where d_satNrgCorr is invalid." ;
    		d_reflctUC:source = "Rel 34 GLAS Binary Data" ;
    		d_reflctUC:coordinates = "DS_UTCTime_40" ;
    		d_reflctUC:valid_min = 0. ;
    		d_reflctUC:valid_max = 1. ;
    		d_reflctUC:_FillValue = 1.79769313486232e+308 ;
    	double d_sDevNsOb1(DS_UTCTime_40) ;
    		d_sDevNsOb1:units = "volts" ;
    		d_sDevNsOb1:hertz = "40" ;
    		d_sDevNsOb1:long_name = "Standard deviation of 1064 nm Background noise, (alternate)" ;
    		d_sDevNsOb1:description = "The standard deviation of the background noise (alternate parameters)." ;
    		d_sDevNsOb1:source = "Rel 34 GLAS Binary Data" ;
    		d_sDevNsOb1:coordinates = "DS_UTCTime_40" ;
    		d_sDevNsOb1:valid_min = 0. ;
    		d_sDevNsOb1:valid_max = 3. ;
    		d_sDevNsOb1:_FillValue = 1.79769313486232e+308 ;
    	double d_satNrgCorr(DS_UTCTime_40) ;
    		d_satNrgCorr:units = "joules" ;
    		d_satNrgCorr:long_name = "Saturation Energy Correction" ;
    		d_satNrgCorr:description = "Correction to energy for saturated waveforms. This correction has not been applied to the energy. It should be ADDED to any echo pulse energy calculated from the pulse area under the waveform. Also any reflectivity estimates need to be corrected for this error in energy measurement." ;
    		d_satNrgCorr:source = "Rel 34 GLAS Binary Data" ;
    		d_satNrgCorr:coordinates = "DS_UTCTime_40" ;
    		d_satNrgCorr:valid_min = 0. ;
    		d_satNrgCorr:valid_max = 1.e-15 ;
    		d_satNrgCorr:_FillValue = 1.79769313486232e+308 ;
    		d_satNrgCorr:hertz = "40" ;
    	double d_RecNrgAll(DS_UTCTime_40) ;
    		d_RecNrgAll:units = "joules" ;
    		d_RecNrgAll:long_name = "Received Energy signal begin to signal end" ;
    		d_RecNrgAll:description = "This is calculated by taking the area under the received waveform (referenced to the observed noise) from all responses between the noise crossing before the first threshold crossing and the noise crossing after the last threshold crossing. It is a rescaled value from the GLA01 parameter d_recNrgAll_EU and is not recomputed." ;
    		d_RecNrgAll:source = "Rel 34 GLAS Binary Data" ;
    		d_RecNrgAll:coordinates = "DS_UTCTime_40" ;
    		d_RecNrgAll:valid_min = 0. ;
    		d_RecNrgAll:valid_max = 3.2e-13 ;
    		d_RecNrgAll:_FillValue = 1.79769313486232e+308 ;
    		d_RecNrgAll:hertz = "40" ;

    // group attributes:
    		:Description = "This group contains reflectivity parameters." ;
    } // group Reflectivity

