group: gt1r {

  // group attributes:
  		:Description = "Each group contains the segments for one Ground Track. As ICESat-2 orbits the earth, sequential transmit pulses illuminate six ground tracks on the surface of the earth.  The track width is approximately 14m.  Each ground track is numbered, according to the laser spot number that generates a given ground track.  Ground tracks are numbered from the left to the right in the direction of spacecraft travel as: 1L, 1R in the left-most pair of beams; 2L, 2R for the center pair of beams; and 3L, 3R for the right-most pair of beams." ;
  		:atlas_pce = "pce3" ;
  		:atlas_beam_type = "strong" ;
  		:groundtrack_id = "gt1r" ;
  		:atmosphere_profile = "profile_1" ;
  		:atlas_spot_number = "5" ;
  		:sc_orientation = "Forward" ;

  group: bckgrd_atlas {
    dimensions:
    	delta_time = 71814 ;
    variables:
    	float bckgrd_int_height(delta_time) ;
    		bckgrd_int_height:contentType = "modelResult" ;
    		bckgrd_int_height:coordinates = "delta_time" ;
    		bckgrd_int_height:description = "The height of the altimetric range window.  This is the height over which the 50-shot sum is generated.  Parameter is ingested at 50-Hz, and values are repeated to form a 200-Hz array." ;
    		bckgrd_int_height:long_name = "Altimetric range window width" ;
    		bckgrd_int_height:source = "ATL03 ATBD Section 7.3" ;
    		bckgrd_int_height:units = "meters" ;
    	float tlm_height_band1(delta_time) ;
    		tlm_height_band1:coordinates = "delta_time" ;
    		tlm_height_band1:long_name = "Height of the telemetry band 1" ;
    		tlm_height_band1:source = "ATL03 ATBD, Section 7.3.2" ;
    		tlm_height_band1:units = "meters" ;
    		tlm_height_band1:description = "The height in meters of the telemetry band 1." ;
    	float bckgrd_rate(delta_time) ;
    		bckgrd_rate:contentType = "modelResult" ;
    		bckgrd_rate:coordinates = "delta_time" ;
    		bckgrd_rate:description = "The background count rate from the 50-shot altimetric histogram after removing the number of likely signal photons based on Section 5." ;
    		bckgrd_rate:long_name = "Background count rate based on the ATLAS 50-shot sum" ;
    		bckgrd_rate:source = "ATL03 ATBD Section 7.3" ;
    		bckgrd_rate:units = "counts / second" ;
    	float tlm_top_band1(delta_time) ;
    		tlm_top_band1:coordinates = "delta_time" ;
    		tlm_top_band1:description = "The ellipsoidal heights with respect to WGS-84 of the top of the telemetry band 1, with all geophysical corrections applied." ;
    		tlm_top_band1:long_name = "Ellipsoidal height of the top of the telemetry band 1." ;
    		tlm_top_band1:source = "ATL03 ATBD, Section 3.2, 7.3.2" ;
    		tlm_top_band1:units = "meters" ;
    	float tlm_top_band2(delta_time) ;
    		tlm_top_band2:coordinates = "delta_time" ;
    		tlm_top_band2:description = "The ellipsoidal heights with respect to WGS-84 of the top of the telemetry band 2, with all geophysical corrections applied." ;
    		tlm_top_band2:long_name = "Ellipsoidal height of the top of the telemetry band 2." ;
    		tlm_top_band2:source = "ATL03 ATBD, Section 3.2, 7.3.2" ;
    		tlm_top_band2:units = "meters" ;
    	float bckgrd_int_height_reduced(delta_time) ;
    		bckgrd_int_height_reduced:contentType = "modelResult" ;
    		bckgrd_int_height_reduced:coordinates = "delta_time" ;
    		bckgrd_int_height_reduced:description = "The height of the altimetric range window after subtracting the height span of the signal photon events in the 50-shot span." ;
    		bckgrd_int_height_reduced:long_name = "Altimetric range window height - reduced" ;
    		bckgrd_int_height_reduced:source = "ATL03 ATBD Section 7.3" ;
    		bckgrd_int_height_reduced:units = "meters" ;
    	float bckgrd_hist_top(delta_time) ;
    		bckgrd_hist_top:contentType = "modelResult" ;
    		bckgrd_hist_top:coordinates = "delta_time" ;
    		bckgrd_hist_top:description = "The height of the top of the altimetric histogram, in meters above the WGS-84 ellipsoid, with all geophysical corrections applied. Parameter is ingested at 50-Hz, and values are repeated to form a 200-Hz array." ;
    		bckgrd_hist_top:long_name = "Top of the altimetric range window" ;
    		bckgrd_hist_top:source = "ATL03 ATBD Section 7.3" ;
    		bckgrd_hist_top:units = "meters" ;
    	int bckgrd_counts_reduced(delta_time) ;
    		bckgrd_counts_reduced:contentType = "modelResult" ;
    		bckgrd_counts_reduced:coordinates = "delta_time" ;
    		bckgrd_counts_reduced:description = "Number of photon counts in the 50-shot sum after subtracting the number of signal photon events, defined as in ATBD Section 5, in that span." ;
    		bckgrd_counts_reduced:long_name = "ATLAS 50-shot background count - reduced" ;
    		bckgrd_counts_reduced:source = "ATL03 ATBD Section 7.3" ;
    		bckgrd_counts_reduced:units = "counts" ;
    	float tlm_height_band2(delta_time) ;
    		tlm_height_band2:coordinates = "delta_time" ;
    		tlm_height_band2:description = "The height in meters of the telemetry band 2. (if 0, second band is not present)." ;
    		tlm_height_band2:long_name = "Height of the telemetry band 2" ;
    		tlm_height_band2:source = "ATL03 ATBD, Section 7.3.2" ;
    		tlm_height_band2:units = "meters" ;
    	double delta_time(delta_time) ;
    		delta_time:description = "Elapsed GPS Seconds from the ATLAS SDP GPS Epoch, referenced to the start of the 50-shot sum. This is based on every fiftieth laser fire time, which leads to a very close alignment with major frame boundaries (+/- 1 shot). The ATLAS Standard Data Products (SDP) epoch offset is defined within /ancillary_data/atlas_sdp_gps_epoch as the number of GPS seconds between the GPS epoch (removed date-time) and the ATLAS SDP epoch. By adding the offset contained within atlas_sdp_gps_epoch to delta time parameters, the time in gps_seconds relative to the GPS epoch can be computed." ;
    		delta_time:long_name = "Time at the start of ATLAS 50-shot sum" ;
    		delta_time:source = "ATL02" ;
    		delta_time:standard_name = "time" ;
    		delta_time:units = "seconds since 2018-01-01" ;
    	int64 pce_mframe_cnt(delta_time) ;
    		pce_mframe_cnt:coordinates = "delta_time" ;
    		pce_mframe_cnt:description = "Major Frame ID - The major frame ID is read from the DFC and starts counting at DFC POR. The counter is used to identify individual major frames across diag and science packets. This counter can go for about 2.7 years before rolling over. It is in the first time tag science packet. Used as part of the photon ID and the safest way to align data within different APIDs or at different rates." ;
    		pce_mframe_cnt:long_name = "PCE Major frame counter" ;
    		pce_mframe_cnt:source = "ATL02" ;
    		pce_mframe_cnt:units = "counts" ;
    	int bckgrd_counts(delta_time) ;
    		bckgrd_counts:contentType = "modelResult" ;
    		bckgrd_counts:coordinates = "delta_time" ;
    		bckgrd_counts:description = "Onboard 50 shot background (200 Hz) sum of photon events within the altimetric range window." ;
    		bckgrd_counts:long_name = "ATLAS 50-shot background count" ;
    		bckgrd_counts:source = "ATL03 ATBD Section 7.3" ;
    		bckgrd_counts:units = "counts" ;

    // group attributes:
    		:Description = "Contains data related to the 50-shot background count, including telemetry and range windows." ;
    } // group bckgrd_atlas

  group: geolocation {
