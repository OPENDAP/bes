    <Int64 name="total_events">
        <Dim size="20"/>
        <Attribute name="contentType" type="String">
            <Value>auxiliaryInformation</Value>
        </Attribute>
        <Attribute name="description" type="String">
            <Value>Number of events used in constructing the per-channel histogram</Value>
        </Attribute>
        <Attribute name="long_name" type="String">
            <Value>Total Events</Value>
        </Attribute>
        <Attribute name="source" type="String">
            <Value>CAL20</Value>
        </Attribute>
        <Attribute name="units" type="String">
            <Value>1</Value>
        </Attribute>
        <dmrpp:compact>EqsBAAAAAAC5xwEAAAAAAPDGAQAAAAAAyawBAAAAAAByuwEAAAAAAHvjAQAAAAAAdOUBAAAAAAAy1gEAAAAAAJfUAQAAAAAASdwBAAAAAAC/zwEAAAAAAC/BAQAAAAAAia4BAAAAAACxvAEAAAAAAAPZAQAAAAAAc7QBAAAAAAAwlwEAAAAAADKpAQAAAAAAZbwBAAAAAAA+yAEAAAAAAA==</dmrpp:compact>
