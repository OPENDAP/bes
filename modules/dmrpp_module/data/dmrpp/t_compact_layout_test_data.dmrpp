    <!-- Float64   - - - - - - - - - - - - - - - - - - - - - - - - -->
    <Float64 name="delta_t_lin_fit">
        <Dim size="5"/>
        <Attribute name="contentType" type="String">
            <Value>auxiliaryInformation</Value>
        </Attribute>
        <Attribute name="description" type="String">
            <Value>Time span over which to perform a running linear fit to identified signal photon events when editing outliers. Surface type dependent.</Value>
        </Attribute>
        <Attribute name="long_name" type="String">
            <Value>Linear fit time increment</Value>
        </Attribute>
        <Attribute name="source" type="String">
            <Value>ATL03, Section 5, _t_linfit_edit</Value>
        </Attribute>
        <Attribute name="units" type="String">
            <Value>seconds</Value>
        </Attribute>
        <dmrpp:compact>rUz4pX7eyD/h0cYRa/HFP6yt2F92T8Y/xT2WPnRBrT/FPZY+dEGtPw==</dmrpp:compact>
    </Float64>


    <!-- Float32   - - - - - - - - - - - - - - - - - - - - - - - - -->
    <Float32 name="tep_bin_size">
        <Dim size="1"/>
        <Attribute name="contentType" type="String">
            <Value>auxiliaryInformation</Value>
        </Attribute>
        <Attribute name="description" type="String">
            <Value>Size of each TEP histogram bin.</Value>
        </Attribute>
