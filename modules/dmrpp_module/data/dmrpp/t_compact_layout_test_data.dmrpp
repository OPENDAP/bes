        <dmrpp:FixedLengthStringArray string_length="80" pad="null_term" />
    </String>

    <!-- String  - - - - - - - - - - - - - - - - - - - - - - - - -->
    <String name="version">
        <Dim size="1"/>
        <Attribute name="contentType" type="String">
            <Value>auxiliaryInformation</Value>
        </Attribute>
        <Attribute name="description" type="String">
