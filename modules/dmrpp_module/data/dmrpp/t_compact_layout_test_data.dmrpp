    <!-- Int8   - - - - - - - - - - - - - - - - - - - - - - - - -->
    <Int8 name="tep_valid_spot">
        <Dim size="6"/>
        <Attribute name="contentType" type="String">
            <Value>referenceInformation</Value>
        </Attribute>
        <Attribute name="description" type="String">
            <Value>A 6x1 array indicating which TEP to use for each spot that does not have a TEP associated with it (e.g. which TEP to use to characterize spots 2, 4, 5, and 6).</Value>
        </Attribute>
        <Attribute name="flag_meanings" type="String">
            <Value>pce1_spot1 pce2_spot3</Value>
        </Attribute>
        <Attribute name="flag_values" type="Int8">
            <Value>1</Value>
            <Value>2</Value>
        </Attribute>
        <Attribute name="long_name" type="String">
            <Value>Index of TEP Spot</Value>
        </Attribute>
        <Attribute name="source" type="String">
            <Value>ATL03 ATBD</Value>
        </Attribute>
        <Attribute name="units" type="String">
            <Value>1</Value>
        </Attribute>
        <Attribute name="valid_max" type="Int8">
            <Value>2</Value>
        </Attribute>
        <Attribute name="valid_min" type="Int8">
            <Value>1</Value>
        </Attribute>
        <dmrpp:compact>AgEBAgIB</dmrpp:compact>
    </Int8>

