    <!-- Int32   - - - - - - - - - - - - - - - - - - - - - - - - -->
    <Int32 name="nbin_min">
        <Dim size="5"/>
        <Attribute name="contentType" type="String">
            <Value>auxiliaryInformation</Value>
        </Attribute>
        <Attribute name="description" type="String">
            <Value>Minimum number of bins in a histogram required for the algorithm to be able to process the histogram.</Value>
        </Attribute>
        <Attribute name="long_name" type="String">
            <Value>Minimum number of bins</Value>
        </Attribute>
        <Attribute name="source" type="String">
            <Value>ATL03, Section 5, Nbinmin</Value>
