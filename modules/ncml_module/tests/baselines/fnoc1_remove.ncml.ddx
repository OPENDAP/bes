<?xml version="1.0" encoding="ISO-8859-1"?>
<Dataset name="fnoc1_remove.ncml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xml.opendap.org/ns/DAP2" xsi:schemaLocation="http://xml.opendap.org/ns/DAP2 http://xml.opendap.org/dap/dap2.xsd">
    <Attribute name="NC_GLOBAL" type="Container">
