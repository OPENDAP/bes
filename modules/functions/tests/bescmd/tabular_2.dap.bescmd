<?xml version="1.0" encoding="UTF-8"?>
<bes:request xmlns:bes="http://xml.opendap.org/ns/bes/1.0#" reqID="[http-8080-1:27:bes_request]" reqUUID="SomeUUIDString">
  <bes:setContext name="xdap_accept">3.2</bes:setContext>
  <bes:setContext name="dap_explicit_containers">no</bes:setContext>
  <bes:setContext name="errors">xml</bes:setContext>
  <bes:setContext name="max_response_size">0</bes:setContext>
  
  <bes:setContainer name="c" space="catalog">/functions/tests/data/fnoc.dods</bes:setContainer>
  
  <bes:define name="d1" space="default">
    <bes:container name="c">
      <!-- tabular(lat) & lat < 20 (it seems the spaces don't break my parser)-->
      <bes:dap4function>tabular(lat) &amp; lat &lt; 20</bes:dap4function>
    </bes:container>
  </bes:define>
  
  <bes:get type="dap" definition="d1" />
  
</bes:request>
