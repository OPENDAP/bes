
BES.ServerAdministrator=admin.email.address@your.domain.name

BES.User=user_name
BES.Group=group_name

BES.LogName=./bes.log
BES.LogVerbose=no

BES.modules=dap,cmd,dapreader,httpd,nc
BES.module.dap=@abs_top_builddir@/dap/.libs/libdap_module.so
BES.module.cmd=@abs_top_builddir@/xmlcommand/.libs/libdap_xml_module.so
BES.module.dapreader=@abs_top_builddir@/dapreader/.libs/libdapreader_module.so
BES.module.nc=@abs_top_builddir@/modules/netcdf_handler/.libs/libnc_module.so

BES.module.httpd=@abs_top_builddir@/modules/httpd_catalog_module/.libs/libhttpd_catalog_module.so


#################################################################################
# Default Catalog 
BES.Catalog.Default=catalog
BES.Catalog.catalog.RootDirectory=@abs_top_srcdir@/modules/httpd_catalog_module

BES.Data.RootDirectory=/dev/null

BES.Catalog.catalog.TypeMatch=dapreader:.*\.das$;dapreader:.*\.dods$;nc:.*\.nc(4)?(\.bz2|\.gz|\.Z)?$
BES.Catalog.catalog.Include=;
BES.Catalog.catalog.Exclude=^\..*;
BES.Catalog.catalog.FollowSymLinks=No

BES.Info.Buffered=no
BES.Info.Type=xml

BES.UncompressCache.dir=/tmp
BES.UncompressCache.prefix=uncompress_cache
BES.UncompressCache.size=500


#-----------------------------------------------------------------------#
#                                                                       #
# httpd_catalog Specific key/value parameters                            #
#                                                                       #
#-----------------------------------------------------------------------#

# Need to white list the test server so we can talk to it.
Gateway.Whitelist=http://localhost
Gateway.Whitelist+=http://test.opendap.org

# Configure data cache
HttpResourceCache.dir=@abs_top_builddir@/modules/httpd_catalog_module/tests/cache
HttpResourceCache.prefix=httpd_cat_
HttpResourceCache.size=500

BES.Catalog.RemoteResources.RootDirectory=UNUSED
BES.Catalog.RemoteResources.TypeMatch=unused:*;

Httpd_Catalog.MimeTypes=nc:application/x-netcdf
Httpd_Catalog.MimeTypes+=h4:application/x-hdf
Httpd_Catalog.MimeTypes+=h5:application/x-hdf5

Httpd_Catalog.Collections=test:http://test.opendap.org/data/
